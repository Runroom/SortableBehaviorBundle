{
    "name": "runroom/sortable-behavior-bundle",
    "type": "symfony-bundle",
    "description": "Provides a way to sort your admin listing",
    "keywords": ["sortable", "admin", "sonata"],
    "homepage": "https://github.com/runroom/SortableBehaviorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "runroom@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/common": "^2.10",
        "symfony/config": "^3.4 || ^4.2",
        "symfony/dependency-injection": "^3.4 || ^4.2",
        "symfony/http-foundation": "^3.4 || ^4.2",
        "symfony/http-kernel": "^3.4 || ^4.2",
        "symfony/property-access": "^3.4 || ^4.2",
        "symfony/translation": "^3.4 || ^4.2",
        "twig/twig": "^2.7"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "gedmo/doctrine-extensions": "^2.4",
        "sonata-project/admin-bundle": "^3.48"
    },
    "suggest": {
        "sonata-project/admin-bundle": "Can be easily integrated with Sonata Admin bundle",
        "stof/doctrine-extensions-bundle": "Manage doctrine extensions with Symfony"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\SortableBehaviorBundle\\": "src/"
        }
    }
}
